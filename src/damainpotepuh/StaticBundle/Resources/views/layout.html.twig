<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
  <head>
    <meta charset="utf-8">
    <title>{% block title %}{% endblock %}Dama in potepuh</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">
    <meta name="author" content="Sano Fužir">

    
    {% stylesheets output='css/style.css' filter=''
           '@damainpotepuhStaticBundle/Resources/assets/css/bootstrap.css'
           '@damainpotepuhStaticBundle/Resources/assets/css/bootstrap-responsive.css'
           '@damainpotepuhStaticBundle/Resources/assets/css/style.css'
           '@damainpotepuhStaticBundle/Resources/assets/css/lightbox.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

  </head>

  <body>
      
  
<div id="wrap">
    <div class="navbar navbar-damainpotepuh navbar-fixed-top">
      <div class="navbar-damainpotepuh navbar-inner">
        <div class="container">
          <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="brand" href="{{ path('_home') }}">
                <img src="{{ asset('bundles/damainpotepuhstatic/images/logo.png') }}" alt="Logo" id="logo"/>
          </a>          
          <div class="nav-collapse-damainpotepuh nav-collapse collapse">
              <ul class="nav">
                <li style="margin-left: 30px;"><a href="{{ path('_home') }}" style="color: #fff;">DOMOV</a></li>
                <li style="margin-left: 33px;"><a href="{{ path('_o_nas') }}" style="color: #fff;">O NAS</a></li>
                 <li class="dropdown"  style="margin-left: 33px;">
                  <a href="{{ path('_artikli') }}" class="dropdown-toggle" data-toggle="dropdown" style="color: #fff;">ARTIKLI <b class="caret"></b></a>
                  <ul class="damainpotepuh-dropdown-menu dropdown-menu">
                    <li><a href="{{ path('_dogs') }}" style="color: #fff;">PSI</a></li>
                    <li><a href="{{ path('_cats') }}" style="color: #fff;">MAČKE</a></li>
                    <li><a href="{{ path('_birds') }}" style="color: #fff;">PTICE</a></li>
                    <li><a href="{{ path('_rodents') }}" style="color: #fff;">GLODALCI</a></li>
                    <li><a href="{{ path('_fishes') }}" style="color: #fff;">RIBE</a></li>
                    <li class="divider"></li>
                    <li><a href="{{ path('_action_products') }}" style="color: #fff;">AKCIJSKI IZDELKI</a></li>
                    <li><a href="{{ path('_new_products') }}" style="color: #fff;">NOVI IZDELKI</a></li>
                    <li><a href="{{ path('_fancy') }}" style="color: #fff;">ŠMENSI-FENSI</a></li>
                  </ul>
                </li>
                <li style="margin-left: 33px;"><a href="{{ path('_galerija') }}" style="color: #fff;">GALERIJA</a></li>
                <li style="margin-left: 33px;"><a href="{{ path('_kontakt') }}" style="color: #fff;">KONTAKT</a></li>
                <li style="margin-left: 33px;">
                    <img src="{{ asset('bundles/damainpotepuhstatic/images/heart.jpg') }}" alt="Heart" id="heart" />
                    <a href="{{ path('_zmenkarije') }}" style="color: #fff; margin-top: -20px;">ZMENKARIJE</a>
                </li>
              </ul>
            </div><!--/.nav-collapse -->
          
        </div>
      </div>
    </div>

    <div class="container">
      {% for type, messages in app.session.flashbag.all() %}
          {% for message in messages %}
              <div class="alert alert-{{type}} fade in" data-alert="alert"><a class="close" data-dismiss="alert" href="#">&times;</a><div class="alert-heading">{{ message }}</div></div>
          {% endfor %}
      {% endfor %}    
                  
        <div class="symfony-content">
            {% block content %}
            {% endblock %}
        </div>
    </div>      
        
    <div id="push"></div>
              
</div>
    <div id="footer">
        <div class="container">
            <hr />
            <p class="damainpotepuh-footer muted credit">
                <span style="background-color: #F9D49E; padding: 0px 5px;">
                    Dama in potepuh - Trgovina za male živali, Nina Grabnar s.p. Dolenska cesta 149, 1000 Ljubljana
                </span>
            </p>            
        </div>
    </div>     
    
      
    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    {% javascripts  output='js/scripts.js'
        '@damainpotepuhStaticBundle/Resources/assets/js/vendor/jquery-1.7.2.js'
        '@damainpotepuhStaticBundle/Resources/assets/js/vendor/lightbox.js'
        '@damainpotepuhStaticBundle/Resources/assets/js/vendor/bootstrap.js'
        '@damainpotepuhStaticBundle/Resources/assets/js/*' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script>
      !function ($) {
        $(function(){
          // carousel demo
          $('#myCarousel').carousel()
        })
      }(window.jQuery)
    </script>

  </body>
</html>
